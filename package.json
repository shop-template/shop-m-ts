{
  "name": "shop-m-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "devF": "vite --force",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint --ext .vue,.js,.ts,.tsx ./src --max-warnings 0 --fix",
    "lint": "eslint --ext .vue,.js,.ts,.tsx ./src --max-warnings 0",
    "cz": "czg",
    "changelog": "node ./updateVersion.js && conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vant/area-data": "^1.3.1",
    "@vant/use": "^1.4.1",
    "@vueuse/core": "^8.7.5",
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1",
    "compressorjs": "^1.1.1",
    "dayjs": "^1.11.3",
    "lodash.merge": "^4.6.2",
    "pinia": "^2.0.14",
    "signature_pad": "^4.0.5",
    "swiper": "^8.2.6",
    "trim-canvas": "^0.1.2",
    "universal-cookie": "^4.0.4",
    "vant": "^3.5.2",
    "vconsole": "^3.14.6",
    "vue": "^3.2.25",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/lodash.merge": "^4.6.7",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "cz-git": "^1.3.8",
    "czg": "^1.3.8",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.7.1",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.20.1",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
